@using Web.Extensions.HtmlHelperExtensions
@using Web.ViewModels.DetailViews.ProductDetailViews

@model ContactDetailViewModel
@{
    ViewBag.Title = "Edit Contact";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <h2>New Contact</h2>
    @Html.WithButton("backButton", "Back").DetailViewFor(typeof(ContactDetailViewModel), "contactDetailView", "Submit")
</div>
@section scripts{
    <script>
        var currentPage = "#ContactNav";

        $(document).ready(function () {
            contactController.init('@Html.GetWebServerUri()');
        });
    </script>
}